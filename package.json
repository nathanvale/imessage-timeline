{
  "name": "imessage-timeline",
  "version": "0.0.0",
  "description": "Extract, transform, and analyze iMessage conversations with AI-powered enrichment and timeline rendering.",
  "type": "module",
  "imports": {
    "#schema/*": "./src/schema/*",
    "#ingest/*": "./src/ingest/*",
    "#normalize/*": "./src/normalize/*",
    "#enrich/*": "./src/enrich/*",
    "#render/*": "./src/render/*"
  },
  "bin": {
    "imessage-timeline": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist .tsbuildinfo 2>/dev/null || true",
    "dev": "pnpm build && node --env-file=.env ./dist/cli.js",
    "start": "node --env-file=.env ./dist/cli.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier -w .",
    "prepare": "husky",
    "version:gen": "changeset",
    "release": "changeset publish",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "validate:json": "node .scripts/validate-json.mjs",
    "commitlint": "commitlint --edit $GIT_PARAMS || true",
    "quality-check": "quality-check",
    "quality-check:ci": "quality-check --since origin/main --safe --fix"
  },
  "engines": {
    "node": ">=22.20"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "license": "MIT",
  "keywords": [
    "imessage",
    "typescript",
    "cli",
    "data-pipeline",
    "message-analysis",
    "timeline",
    "ai-enrichment"
  ],
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@mendable/firecrawl-js": "^4.3.7",
    "commander": "^14.0.1",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "sharp": "^0.34.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "better-sqlite3": "^12.4.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.38.0",
    "@orchestr8/quality-check": "^1.0.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.8.1",
    "@vitest/coverage-v8": "^3.2.4",
    "changesets": "^1.0.2",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.1",
    "prettier": "^3.6.2",
    "prettier-plugin-sort-json": "^4.1.1",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^3.2.4"
  }
}
